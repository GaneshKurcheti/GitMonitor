<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="EnableLog" value="true"/>
    <add key="LogPath" value="C:\"/>
    <add key="SimultaneousCheckCount" value="10"/>
    <add key="RunInterval" value="10"/>
    <add key="IsProduction" value="false"/>
    <add key="TestMail" value="test1@gmail.com;test2@gmail.com"/>
  </appSettings>

  <system.net>
    <mailSettings>
      <smtp from="test@gmail.com">
        <network host="smtp.gmail.com" port="587" userName="test@gmail.com" password="test@1234" enableSsl="true" />
      </smtp>
    </mailSettings>
  </system.net>

</configuration>